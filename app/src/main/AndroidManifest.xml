<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:name=".LogApplication"
        android:allowBackup="true"
        android:dataExtractionRules="@xml/data_extraction_rules"
        android:fullBackupContent="@xml/backup_rules"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/Theme.Binarecommerce"
        tools:targetApi="31">
        <activity
            android:name=".view.ui.HistoryActivity"
            android:exported="false" />
        <activity
            android:name=".view.ui.CartActivity"
            android:exported="false" />
        <activity
            android:name=".view.ui.FavoriteActivity"
            android:exported="false" />
        <activity
            android:name=".view.ui.ProfileActivity"
            android:exported="false" />
        <activity
            android:name=".view.ui.RegisterActivity"
            android:exported="true" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".view.ui.DetailActivity"
            android:exported="false" />
        <activity
            android:name=".view.ui.LoginActivity"
            android:exported="false" />
        <activity
            android:name=".view.ui.HomeActivity"
            android:exported="false" />
        <activity
            android:name=".MainActivity"
            android:exported="true">
        </activity>
    </application>

</manifest>